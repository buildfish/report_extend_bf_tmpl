<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Report Simple Account Invoice -->
        <report id="action_report_my_account_invoice"
            model="account.move"
            string="Invoice basic"
            report_type="controller"
            name="MyAccountInvoice"/>
        <record id="action_report_my_account_invoice" model="ir.actions.report">
            <field name="template_id" ref="report_extend_bf_demo_account.account_invoice_001"/>
            <field name="output_file">pdf</field>
        </record>

        <!-- Report Custom Account Invoice -->
        <report id="action_report_my_account_invoice_custom"
            model="account.move"
            string="My invoice special custom"
            report_type="controller"
