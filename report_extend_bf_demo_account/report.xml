            <field name="template_id" ref="report_extend_bf_demo_account.account_invoice_002"/>
            <field name="output_file">doc</field>
        </record>

        <!-- Report terms &amp; conditions -->
        <report id="action_report_terms_conditions"
            model="account.move"
            string="Report terms &amp; conditions"
            report_type="controller"
            name="TermsConditions"/>
        <record id="action_report_terms_conditions" model="ir.actions.report">
            <field name="template_id" ref="report_extend_bf_demo_account.account_invoice_003"/>
            <field name="output_file">pdf</field>
